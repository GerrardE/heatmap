{
  "name": "heatmap-frontend",
  "version": "1.0.0",
  "description": "heatmap lets you have a view of your financial operations over the year",
  "main": "app.js",
  "scripts": {
    "start:dev": "webpack-dev-server --config webpack.dev.js --open",
    "build": "npm run clean && webpack --config webpack.prod.js",
    "clean": "rm -rf dist",
    "heroku-postbuild": "npm run build",
    "test": "jest --coverage",
    "test:dev": "jest --coverage && coverage",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "test:watch": "jest --coverage --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GerrardE/heatmap-frontend.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack"
  ],
  "author": "Gerrard Ezeugwa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GerrardE/heatmap-frontend"
  },
  "homepage": "https://github.com/GerrardE/heatmap-frontend#readme",
  "devDependencies": {
    "assert": "^2.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "redux-mock-store": "^1.5.3",
    "sinon": "^7.5.0",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "dotenv": "^8.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "lodash-webpack-plugin": "^0.11.4",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prop-types": "^15.7.2",
    "raw-loader": "^3.0.0",
    "react": "^16.8.6",
    "react-calendar-heatmap": "^1.8.1",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-merge": "^4.2.1"
  },
  "engines": {
    "node": "10.15.3",
    "npm": "6.10.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>__tests__/setup/enzyme_setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>__tests__/setup/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>__tests__/setup/assetsTransformer.js"
    }
  }
}
